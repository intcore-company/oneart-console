{
    "name": "markrady/oneart-console",
    "description": "The Command Line Interface for the OneART Architecture",
    "keywords": ["laravel", "intc", "mvc", "ddd", "domains", "architecture"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Rady",
            "email": "me@markrady.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MarkRady\\OneARTConsole\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarkRady\\OneARTConsole\\OneARTServiceProvider"
            ]
        }
    },
    "bin": ["oneart"],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^4.1",
        "symfony/process": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/filesystem": "^4.1",
        "phploc/phploc": "~4.0",
        "illuminate/support" : "^6.6.0",
        "illuminate/config" : "^6.6.0"
    }
}
