{
    "name": "intcore-company/oneart-console",
    "description": "The Command Line Interface for the OneART Architecture",
    "keywords": ["laravel", "intc", "mvc", "ddd", "domains", "architecture"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Rady",
            "email": "mark@intcore.net"

        },
        {
            "name": "intcore",
            "email": "info@intcore.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "INTCore\\OneARTConsole\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "INTCore\\OneARTConsole\\OneARTServiceProvider"
            ]
        }
