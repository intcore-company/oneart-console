    "bin": ["oneart"],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.1.4",
        "symfony/process": "^7.1.3",
        "symfony/finder": "^v7.1.4",
        "symfony/filesystem": "^7.1.2",
        "illuminate/support" : "^11.0.0",
        "illuminate/config" : "^11.0.0"
    }
}
